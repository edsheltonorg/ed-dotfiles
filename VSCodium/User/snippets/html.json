{
	// Place your snippets for html here. Each snippet is defined under a snippet name and has a prefix, body and
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"HTML Boilerplate Extended": {
		"prefix": "!!-Extended",
		"body":[
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"  ",
			"  <head>",
			"    <!-- Boilerplate Headers -->",
			"    <meta charset=\"UTF-8\"/>",
			"    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\"/>",
			"    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\"/>",
			"    ",
			"    <!-- Robots -->",
			"    <meta name=\"robots\" content=\"$9index,$10follow\"/>",
			"    ",
			"    <!-- CSS & JS -->",
			"    <link rel=\"stylesheet\" href=\"/${RELATIVE_FILEPATH/[\\\\]/\\//g}/${11:<-- CSS PATH}\"/>",
			"    <!-- <script src=\"/JAVASCRIPT.js\" defer></script> -->",
			"    ",
			"    <!-- Page Info -->",
			"    <title>${1:TITLE}</title>",
			"    <meta name=\"description\" content=\"${2:DESCRIPTION}\"/>",
			"    <link rel=\"canonical\" href=\"http$4://${5:URL}/${RELATIVE_FILEPATH/[\\\\]/\\//g}${6:<-- REMOVE EXTENSION}\"/>",
			"    ",
			"    <!-- Thumbnail 192px -->",
			"    <link rel=\"icon\" type=\"image/png\" href=\"/${RELATIVE_FILEPATH/[\\\\]/\\//g}/${7:<-- THUMBNAIL PATH}\"/>",
			"    <link rel=\"apple-touch-icon\" type=\"image/png\" href=\"/${RELATIVE_FILEPATH/[\\\\]/\\//g}/${7:<-- THUMBNAIL PATH}\"/>",
			"    ",
			"    <!-- Twitter Card data -->",
			"    <meta name=\"twitter:card\" value=\"$2\"/>",
			"    ",
			"    <!-- Open Graph data-->",
			"    <meta property=\"og:title\" content=\"$1\"/>",
			"    <meta property=\"og:type\" content=\"article\"/>",
			"    <meta property=\"og:url\" content=\"http$4://$5/${RELATIVE_FILEPATH/[\\\\]/\\//g}$6\"/>",
			"    <meta property=\"og:image\" content=\"http$4://$5/${RELATIVE_FILEPATH/[\\\\]/\\//g}$6/${8:IMAGE PATH}\"/>",
			"    <meta property=\"og:description\" content=\"$2\"/>",
			"  </head>",
			"  ",
			"  <body>",
			"    <h1>${12:H1}</h1>",
			"    <p>${13:BODY 1}</p>",
			"  </body>",
			"  ",
			"</html>",
		],
		"description": "HTML Boilerplate",
	},
	"HTML Boilerplate Basic": {
		"prefix": "!!",
		"body":[
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"  <head>",
			"    <title>${1:TITLE}</title>",
			"    <meta name=\"description\" content=\"${2:DESCRIPTION}\"/>",
			"    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\"/>",
			"    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\"/>",
			"    <link rel=\"canonical\" href=\"http$4://${5:URL}/${RELATIVE_FILEPATH/[\\\\]/\\//g}${6:<-- REMOVE EXTENSION}\"/>",
			"    <meta charset=\"UTF-8\"/>",
			"  </head>",
			"  <body>",
			"    <h1>${7:H1}</h1>",
			"    <p>${8:BODY 1}</p>",
			"  </body>",
			"</html>",
		],
		"description": "HTML Boilerplate",
	},
	"emmet-table-template": {
		"prefix": "emmet-table-template",
		"body": [
			"table>(thead>tr>th*${1:ROWS_N})(tbody>tr>td*${2:ROWS_N})",
		],
		"description": "Build variable size table quickly",
	},
	"img-width": {
		"prefix": "img-width",
		"body":[
			"<img src=\"${1:PATH}\" alt=\"${2:DESCRIPTION}\" width=\"${3:WIDTH}\">",
		]
	},
	"a-href-blank": {
		"prefix": "a-href-blank",
		"body":[
			"<a href=\"${1:URL}\" target=\"_blank\" rel=\"noopener noreferrer\">${2:TEXT}</a>"
		]
	},
	"php": {
		"prefix": "php",
		"body":[
			"<?php",
			"    $0",
			"?>"
		]
	},
	"php-single": {
		"prefix": "php-single",
		"body":[
			"<?php $1 ?>$0",
		]
	},
	"php-header-html": {
		"prefix": "php-header-html",
		"body":[
			"<?php header('Content-type: text/html; charset=utf-8'); ?>",
		]
	},
	"php-header-text": {
		"prefix": "php-header-plain",
		"body":[
			"<?php header('Content-type: text/plain; charset=utf-8'); ?>",
		]
	}
}